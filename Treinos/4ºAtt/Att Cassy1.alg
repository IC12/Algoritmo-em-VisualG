algoritmo "cassy01"
//Problema: qual a idade de Cassy?
var
   ano_atual, ano_nasc, idade: inteiro
inicio
      escreva("Em que ano estamos? ")
      leia(ano_atual)
      escreva("Em que ano eu nasci? ")
      leia(ano_nasc)
      idade <- ano_atual - ano_nasc
      escreva("Minha idade será: ", idade)
fimalgoritmo
